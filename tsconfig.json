{
  "compilerOptions": {
    "target": "esnext",
    "module": "None",
    "lib": [
      "esnext",
      "dom"
    ],
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": ["node_modules"]
}